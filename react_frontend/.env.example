# Supabase Configuration
REACT_APP_SUPABASE_URL=your_supabase_url_here
REACT_APP_SUPABASE_KEY=your_supabase_anon_key_here

# Backend API Configuration
REACT_APP_API_BASE=http://localhost:3001
REACT_APP_BACKEND_URL=http://localhost:3001
REACT_APP_FRONTEND_URL=http://localhost:3000
REACT_APP_WS_URL=ws://localhost:3001

# Environment Configuration
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=false

# Server Configuration
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=false
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/health

# Feature Flags
REACT_APP_FEATURE_FLAGS={}
REACT_APP_EXPERIMENTS_ENABLED=false

# Audius API Configuration
# NOTE: The app_name parameter is required by Audius API for streaming
# Replace with your application name (no spaces, use hyphens or underscores)
REACT_APP_AUDIUS_APP_NAME=music-streamer
